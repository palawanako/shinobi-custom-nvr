<!--Multi Monitor Manager-->
<div class="modal medium fade " mid ke id="multi_mon" tabindex="-1" role="dialog" aria-labelledby="multi_monLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
        <div class="modal-header colorBg">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <h4 class="modal-title" id="multi_monLabel"><i class="fa fa-clone"></i> &nbsp; <%- lang['Monitors'] %></h4>
        </div>
        <form class="modal-body colorContent">
            <table class="table table-striped tableData sortable tbl_list">
            		<thead>
            			<tr>
            				<th class="sorttable_nosort" ><div class="checkbox">
            				<input id="multimon_select_all" type="checkbox"><label for="multimon_select_all"></label></div>
            				</th>
            				<th><%-lang['Row']%></th>
            				<th><%-lang['picture']%></th>
            				<th class="sorttable_nosort"><%-lang['Status']%></th>
            				<th><%-lang['Name']%></th>
            				<th><%-lang['Mode']%></th>
            				<th>IP Address</th>
            				<th>Action</th>

            			</tr>
            		</thead>
            		<tbody>
            		</tbody>
            	</table>

        </form>
        <div class="modal-footer colorBg">
            <button type="button" class="btn btn-default pull-left" data-dismiss="modal"><%-lang['Close']%></button>
            <div>
                <a class="btn btn-danger delete"><%-lang['Delete']%></a>
                <a class="btn btn-info save_config"><%-lang['Export']%></a>
                <a class="btn btn-info save_config_csv">csv</a>
                <a class="btn btn-info import_config"><%-lang['Import']%></a>
                <a class="btn btn-info" data-dismiss="modal" monitor="edit"><%-lang['Add New']%></a>
                <a class="btn btn-info setMode" ><%-lang['Groups Tasks']%></a>
            </div>
        </div>
    </div>
  </div>
</div>
<script src="<%-window.libURL%>libs/js/dash2.multimon.js"></script>
